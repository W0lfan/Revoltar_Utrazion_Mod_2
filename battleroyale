this.tick = {
function StatsScoreShip(game) {
  if (game.step % 15 == 0) {
    for (let ship of game.ships) {
      if (!ship.custom.init) {
        ship.custom.init=true;
      }
      ship.set({score:0});
      ship.set({stats:55555555});
      }
    }






function ShipStart(game) {
if (game.step % 30 === 0) {
  for (let ship of game.ships) {
    if (ship.alive !== ship.custom.alive) {
      ship.custom.alive = ship.alive;
      if (ship.alive) {
        ship.set({crystals: 250});
      }
    }
  }
}
}



